    this.setState({\n                error: '결과를 찾지 못했습니다.'\n            })\n            \n        } finally {\n            this.setState({\n                loading : false,\n                result\n            })\n        }\n    }\n\n    render() {\n        const { result, error, loading } = this.state;\n        console.log('check',this.props);\n        console.log('state check',this.state);\n        return <DetailPresenter result={result} error={error} loading={loading} />\n    }\n}","/Users/mz01-mjoo/Documents/GitHub/nomflix/src/Routes/Detail/DetailPresenter.js",[],"/Users/mz01-mjoo/Documents/GitHub/nomflix/src/Components/Section.js",[],"/Users/mz01-mjoo/Documents/GitHub/nomflix/src/Components/Loader.js",[],"/Users/mz01-mjoo/Documents/GitHub/nomflix/src/Components/Message.js",[],"/Users/mz01-mjoo/Documents/GitHub/nomflix/src/Components/Poster.js",["110"],"import React from 'react'\nimport PropTypes from 'prop-types'\nimport styled from 'styled-components'\nimport { Link } from 'react-router-dom'\n\nconst Container = styled.div``;\nconst ImageContainer = styled.div``;\nconst Image = styled.div`\n    background: url(${prop => prop.bgUrl}) no-repeat;\n    height:180px;\n    background-size:cover;\n    background-position:center center;\n    border-radius:4px;\n    margin-bottom:10px;\n    transition:opacity 0.2s linear;\n`;\nconst Title = styled.span``;\n\nconst Poster = ({ title, id, imageUrl, isMovie = false }) => (\n    <Link to={isMovie ? `/movie/${id}` : `/show/${id}`}>\n        <Container>\n            <ImageContainer>\n                <Image bgUrl={`https://image.tmdb.org/t/p/w300/${imageUrl}`} />\n            </ImageContainer>\n            <Title>{title}</Title>\n        </Container>\n    </Link>\n)\n\nexport default Poster \n",{"ruleId":"111","severity":1,"message":"112","line":20,"column":79,"nodeType":"113","messageId":"114","endLine":20,"endColumn":87},{"ruleId":"111","severity":1,"message":"115","line":2,"column":8,"nodeType":"113","messageId":"114","en